{
  "expo": {
    "name": "myracketpartner-mobile",
    "slug": "myracketpartner-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "myracketpartner",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.erjuanlu90.myracketpartnermobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adapti+6352ve-icon.png",
        "backgroundColor": "#ffffff",
        "infoPlist": {
          "NSAppTransportSecurity": {
            "NSAllowsArbitraryLoads": true
          }
        }
      },
      "package": "com.erjuanlu90.myracketpartnermobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.993763932380-chmaa6d2iu7qcf3nvpcco4u1jhb3n7uo"
        }
      ],
      "expo-localization"
    ]
  }
}
